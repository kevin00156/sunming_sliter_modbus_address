TYPE structDigitalInput :
STRUCT
	xX0_SystemRun					: BIT;	//	(X0)	系統啟動
	xX1_SystemStop					: BIT;	//	(X1)	系統停止
	xX2_EMS							: BIT;	//	(X2)	緊急停止
	xX3_MainMotorStart				: BIT;	//	(X3)	主馬達啟動
