	xX14_HydraulicMASwitch			: BIT;	//	(X14)	油壓手自動
	xX15_LeftVaccumHoleMASwitch		: BIT;	//	(X15)	左吸風口手自動
	xX16_RightVaccumHoleMASwitch	: BIT;	//	(X20)	右吸風口手自動
	xX17_LeftMachineOffset			: BIT;	//	(X21)	機台橫移_左
	xX18_RightMachineOffset			: BIT;	//	(X22)	機台橫移_右
	xX19_SpeedMASwitch				: BIT;	//	(X23)	車速手自動
	xX20_KnifeMotorOL				: BIT;	//	(X24)	刀輪馬達過熱
	xX21_CrimperMotorOL				: BIT;	//	(X25)	線輪馬達過熱
