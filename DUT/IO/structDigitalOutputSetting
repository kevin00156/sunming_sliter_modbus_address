TYPE structDigitalOutputSetting :
STRUCT
	xCurrentState			: BIT;//系統狀態
	xInternalControl 		: BIT;//指定測試
	xForceStatus 			: BIT;//指定該bit測試時的狀態是高或低
	xInverse 				: BIT;//指定物理訊號與系統訊號是否反轉
	{attribute 'hide'}
	xBit5					: BIT;
	{attribute 'hide'}
	xBit6					: BIT;
	{attribute 'hide'}
	xBit7					: BIT;
	{attribute 'hide'}
	xBit8					: BIT;
	{attribute 'hide'}
	xBit9					: BIT;
	{attribute 'hide'}
	xBit10					: BIT;
	{attribute 'hide'}
	xBit11					: BIT;
	{attribute 'hide'}
	xBit12					: BIT;
	{attribute 'hide'}
	xBit13					: BIT;
	{attribute 'hide'}
	xBit14					: BIT;
	{attribute 'hide'}
	xBit15					: BIT;
	{attribute 'hide'}
	xBit16					: BIT;

END_STRUCT
END_TYPE
